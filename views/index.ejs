    <link rel="stylesheet" href="/stylesheets/main.css">
  </head>
  <body>

    <div class="container">
      <div id="imgDiv">
        <video muted autoplay playsinline preload="metadata">
          <source src="assets/landing-page-animation-cropped.mp4" type="video/mp4">
          Your browser does not support the video tag.
          <!-- onerror="fallback(parentNode)" -->
          <!-- <img id="image" src="assets/landing-page-animation-cropped.gif"> -->
        </video>

      </div>

      <div class="fade-in content">
        <div class="row">
          <div class="col-md">
            <h1 class="main">Website coming soon..</h1>
          </div>
        </div>
        <div class="row justify-content-md-center">
          <div class="col-xs-12 col-md-6 info">
            Please check out our
            <a href="https://www.instagram.com/studiodoorgallery/?hl=en">
              <strong>Instagram</strong>
            </a>
            and join our mailing list to be kept up to date in the meantime:
          </div>
        </div>

        <form class="" action="/" method="post">
          <div class="form-group">
            <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email" name="email">
            <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
          </div>
          <div class="form-group">
            <button type="submit" class="btn btn-primary">Submit</button>
          </div>
        </form>



        <div class="bottom-link">
          <a  href="https://www.instagram.com/studiodoorgallery/?hl=en"><i class="fab fa-instagram"></i></a>
        </div>


      </div>
    </div>

    <script type="text/javascript" src="/javascript/main.js"></script>

